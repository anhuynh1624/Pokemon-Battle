<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_have_the_same_pokemon</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Pokemon_1__c == Pokemon_2__c,Pokemon_1__c == Pokemon_3__c,
Pokemon_2__c == Pokemon_3__c)</errorConditionFormula>
    <errorMessage>You cannot use the same pokemon for two positions</errorMessage>
</ValidationRule>
